{
  "name": "@devlander/shared-react-native-types",
  "version": "1.0.199",
  "license": "MIT",
  "main": "dist/umd/index.js",
  "react-native": "dist/native/index.js",
  "browser": "dist/umd/index.js",
  "types": "typings/bundle/index.d.ts",
  "module": "dist/esm/index.js",
  "config": {
    "nativeInput": "index",
    "webInput": "index",
    "umdName": "SharedReactNativeTypes"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "timeout": "delay 5",
    "preinstall": "node ./merge-deps.js",
    "rollup:native": "npx rollup -c rollup.config.mjs",
    "rollup:web": "npx rollup -c rollup.config.mjs",
    "rollup": "npx rimraf ./dist && npx rimraf ./typings && yarn run build:index && yarn run timeout   && yarn run rollup:web ",
    "start": "tsdx watch",
    "build:index": "cd ./src && yarn run build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "test": "jest",
    "prepare": "tsdx build",
    "size": "size-limit",
    "patch": "yarn version --new-version patch",
    "analyze": "size-limit --why",
    "start-publish": " yarn run build && yarn run size && yarn run patch && npm publish"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-native-web": "0.x.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Landon Johnson",
  "size-limit": [
    {
      "path": "dist/umd/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/native/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/esm/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/types/index.d.ts",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@devlander/collect-exports-for-bundle": "^1.1.68",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-auto-install": "^3.0.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@size-limit/preset-small-lib": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-native": "^12.3.2",
    "@types/color": "3.0.4",
    "@types/hoist-non-react-statics": "3.3.1",
    "@types/jest": "^29.5.7",
    "@types/lodash.isempty": "4.4.7",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/react-native": "^0.72.5",
    "@types/react-test-renderer": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.8.0",
    "color": "4.2.3",
    "core-js": "3",
    "delay-cli": "2.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jest-dom": "latest",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-testing-library": "latest",
    "eslint-plugin-unused-imports": "3.0.0",
    "hex-to-rgba": "2.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lodash.isempty": "4.4.0",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native-responsive-fontsize": "0.5.1",
    "react-native-web": "^0.19.9",
    "react-test-renderer": "^18.2.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "size-limit": "^10.0.1",
    "ts-jest": "^29.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "lodash.isempty": "4.4.0",
    "react-native-responsive-fontsize": "0.5.1"
  }
}