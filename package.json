{
  "name": "@devlander/shared-react-native-types",
  "version": "1.0.198",
  "description": "Shared npm modules between the design system, and expo app",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "browser": "dist/umd/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "keywords": [
    "react",
    "react-native",
    "expo",
    "typescript",
    "design-system",
    "devlander"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "config": {
    "rollupEntryPoint": "./src/index.tsx",
    "umdName": "SharedReactNativeTypes",
    "esEntry": "dist/es/index.mjs"
  },
  "packageManager": "yarn@3.6.3",
  "scripts": {
    "clean-yarn": "yarn cache clean && rm -rf node_modules && rm yarn.lock && yarn install",
    "pack": "./pack-and-move.sh",
    "install-for-children": "cd ./examples && yarn run get-bundles",
    "prepare-rollup": "cd ./rollup-config && tsc",
    "docs": "typedoc --out ./docs ./src",
    "remove-unused-packages": "node ./external-modules/remove-unused-packages.js",
    "build:index": "node ./external-modules/auto-export && yarn run prettier",
    "increment-version": "yarn version --patch",
    "prettier": "prettier --write ./src/**/*.{ts,json}",
    "lint": "yarn run prettier && eslint ./src --ext .ts,.tsx --fix",
    "build:esm": "rm -rf ./types && rm -rf ./dist && tsc",
    "timeout": "delay 5",
    "push-main": "git add -A && yarn run timeout && git commit -m 'updated build' && yarn run timeout  && git push origin main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "patch": "yarn version --new-version patch",
    "format": "eslint './src/**/*.ts' --fix && prettier --write './src/**/*.ts'",
    "typecheck": "tsc --project tsconfig.json --noEmit --emitDeclarationOnly false",
    "buildESM": "tsc --project tsconfig.json",
    "buildCJS": "rollup -c",
    "build": "yarn run clean-yarn && yarn run prepare-rollup && yarn run docs &&yarn run build:index && npx rimraf ./dist &&  yarn run format && yarn run typecheck && yarn run buildESM && yarn run buildCJS && yarn run patch && yarn run pack"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/runtime-corejs3": "^7.23.2",
    "@emotion/is-prop-valid": "^1.2.1",
    "@emotion/memoize": "^0.8.1",
    "@emotion/unitless": "^0.8.1",
    "camelize": "^1.0.1",
    "color": "^4.2.3",
    "color-convert": "^2.0.1",
    "color-name": "^2.0.0",
    "color-string": "^1.9.1",
    "core-js": "3.33.1",
    "css-color-keywords": "^1.0.0",
    "css-to-react-native": "^3.2.0",
    "hex-to-rgba": "^2.0.1",
    "is-arrayish": "^0.3.2",
    "lodash.isempty": "^4.4.0",
    "nanoid": "^5.0.2",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.31",
    "postcss-value-parser": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.72.6",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-responsive-fontsize": "0.5.1",
    "shallowequal": "^1.1.0",
    "simple-swizzle": "^0.2.2",
    "source-map-js": "^1.0.2",
    "styled-components": "^6.1.0",
    "stylis": "^4.3.0"
  },
  "author": "devlander-software",
  "license": "ISC",
  "peerDependencies": {
    "@babel/core": "^7.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-native": ">=0.42.0",
    "react-native-web": "^0.19.9"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true,
      "suggested": true
    },
    "react-dom": {
      "optional": true,
      "suggested": true
    },
    "react-native": {
      "optional": true
    },
    "react-native-web": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/helper-create-regexp-features-plugin": "^7.22.15",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@devlander/collect-exports-for-bundle": "^1.0.94",
    "@rollup/plugin-auto-install": "^3.0.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/color": "^3.0.4",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^29.5.5",
    "@types/lodash.isempty": "^4.4.7",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-native": "^0.72.5",
    "@types/rollup-plugin-peer-deps-external": "^2.2.3",
    "@types/styled-components": "^5.1.26",
    "@types/styled-components-react-native": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "add": "^2.0.6",
    "babel-plugin-styled-components": "^2.1.4",
    "delay-cli": "2.0.0",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest-dom": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "latest",
    "eslint-plugin-testing-library": "latest",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.0.1",
    "rollup": "^3.0.0",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-url": "^3.0.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.2",
    "typescript": "5.2.2"
  }
}
