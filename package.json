{
  "name": "@devlander/shared-react-native-types",
  "version": "1.0.176",
  "description": "Shared npm modules between the design system, and expo app",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "browser": "dist/umd/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "keywords": [
    "react",
    "react-native",
    "expo",
    "typescript",
    "design-system",
    "devlander"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@3.6.3",
  "scripts": {
    "build:index": "node ./external-modules/auto-export && yarn run prettier",
    "increment-version": "yarn version --patch",
    "prettier": "prettier --write ./src/**/*.{ts,json}",
    "lint": "yarn run prettier && eslint ./src --ext .ts,.tsx --fix",
    "build:esm": "rm -rf ./types && rm -rf ./dist && tsc",
    "timeout": "delay 5",
    "push-main": "git add -A && yarn run timeout && git commit -m 'updated build' && yarn run timeout  && git push origin main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "patch": "yarn version --new-version patch",
    "format": "eslint './src/**/*.ts' --fix && prettier --write './src/**/*.ts'",
    "typecheck": "tsc --project tsconfig.json --noEmit --emitDeclarationOnly false",
    "buildESM": "tsc --project tsconfig.json",
    "buildCJS": "rollup -c",
    "build": "yarn run build:index && npx rimraf ./dist &&  yarn run format && yarn run typecheck && yarn run buildESM && yarn run buildCJS && yarn run patch"
  },
  "dependencies": {
    "@devlander/collect-exports-for-bundle": "^1.0.63",
    "@devlander/react": "gist:a773a60265123568189d94d242924f38",
    "@devlander/rollup": "gist:85a0cc9660e26ce4f6bf9629da1ca5c7",
    "@devlander/sync-dep-versions": "gist:6c444d07e8686711347fa474dd5540f5",
    "@swc/core": "^1.3.84",
    "color": "^4.2.3",
    "hex-to-rgba": "^2.0.1",
    "jest": "^29.7.0",
    "lodash.isempty": "^4.4.0",
    "react-native": "^0.72.0",
    "react-native-responsive-fontsize": "0.5.1",
    "react-responsive": "^9.0.2",
    "styled-components": "6.0.7",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "5.2.2"
  },
  "overrides": {
    "react-refresh": "~0.14.0"
  },
  "resolutions": {
    "react-refresh": "~0.14.0"
  },
  "author": "devlander-software",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-auto-install": "^3.0.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/color": "^3.0.4",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^29.5.5",
    "@types/lodash.isempty": "^4.4.7",
    "@types/node": "^20.6.0",
    "@types/react": "18.2.0",
    "@types/react-native": "^0.72.3",
    "@types/styled-components": "^5.1.26",
    "@types/styled-components-react-native": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "delay-cli": "2.0.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest-dom": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "latest",
    "eslint-plugin-testing-library": "latest",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.0.1",
    "rollup": "^3.29.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-generate-git-version": "^1.0.0",
    "typedoc": "^0.25.2"
  }
}
